{
  "kind": "collectionType",
  "collectionName": "stock",
  "info": {
    "name": "Stock",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false,
    "exportable": true
  },
  "attributes": {
    "producto": {
      "model": "productos"
    },
    "espesor": {
      "type": "string"
    },
    "ancho": {
      "type": "string"
    },
    "largo": {
      "type": "string"
    },
    "calidad": {
      "type": "enumeration",
      "enum": [
        "Primera",
        "Segunda",
        "Tercera"
      ]
    },
    "volumen_stock": {
      "type": "enumeration",
      "enum": [
        "Pie",
        "M3"
      ]
    },
    "cantidad": {
      "type": "float"
    },
    "especie": {
      "model": "especie"
    },
    "comentarios": {
      "type": "richtext"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "imagenes": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    }
  }
}
