{
  "kind": "collectionType",
  "collectionName": "productos",
  "info": {
    "name": "Producto",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "nombre": {
      "type": "string",
      "required": true
    },
    "descripcion": {
      "type": "richtext"
    },
    "tipo": {
      "model": "productos"
    },
    "usuario": {
      "type": "string",
      "private": true
    },
    "foto": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    }
  }
}
